	<xs:complexType name="ENVELOPE_RESPOND_TO_PARTY_Type">
		<xs:sequence>
			<xs:element name="CONTACT_DETAIL" type="ENVELOPE_CONTACT_DETAIL_Type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="_Name" type="xs:string">
			<xs:annotation>
				<xs:documentation>Identifies the name of the party who is the intended recipient of the XML response message.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Response Group</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Respond To Party Name</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_StreetAddress" type="xs:string">
			<xs:annotation>
				<xs:documentation>The unstructured (Unparsed) street address of the party the response is being sent to (e.g., 123 Main Street).</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Response Group</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Respond To Party Street Address</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_StreetAddress2" type="xs:string">
			<xs:annotation>
				<xs:documentation>The unstructured (Unparsed) street address 2nd division of the party the response is being sent to (e.g., Unit 1223).</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Response Group</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Respond To Party Street Address 2</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_City" type="xs:string">
			<xs:annotation>
				<xs:documentation>The city in which the party the response is being sent to is located.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Response Group</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Respond To Party City</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_State" type="xs:string">
			<xs:annotation>
				<xs:documentation>The state in which the party the response is being sent to is located.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Response Group</mismo:CONTEXT>
