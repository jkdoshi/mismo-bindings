	<xs:element name="REQUEST_GROUP" type="ENVELOPE_REQUEST_GROUP_Type"/>
	<xs:complexType name="ENVELOPE_CONTACT_DETAIL_Type">
		<xs:sequence>
			<xs:element name="CONTACT_POINT" type="ENVELOPE_CONTACT_POINT_Type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="_Name" type="xs:string">
			<xs:annotation>
				<xs:documentation>The unparsed name of an individual acting as a contact for a party to the loan.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT>Contact Detail</mismo:CONTEXT>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Service ordering</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Closing</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Credit Reporting</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Flood</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Application</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Secondary Delivery</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Servicing</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Appraisal</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Servicing Transfer</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Investor Reporting</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>AUS</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>PRIA</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>MI</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Valuation</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Contact Detail Name</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_Identifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>This attribute may be used as a unique identifier for the contact type, as determined by trading partners.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Valuation</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Contact Detail Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_FirstName" type="xs:string">
			<xs:annotation>
				<xs:documentation>The first name of an individual acting as a contact for a party to the loan.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Valuation</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Contact Detail First Name</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_LastName" type="xs:string">
			<xs:annotation>
				<xs:documentation>The last name of an individual acting as a contact for a party to the loan.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Valuation</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Contact Detail Last Name</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_MiddleName" type="xs:string">
			<xs:annotation>
				<xs:documentation>The middle name of an individual acting as a contact for a party to the loan.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Valuation</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Contact Detail Middle Name</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_NameSuffix" type="xs:string">
			<xs:annotation>
				<xs:documentation>The name suffix of an individual acting as a contact for a party to the loan.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Valuation</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Contact Detail Name Suffix</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_SequenceIdentifier" type="xs:string">
			<xs:annotation>
				<xs:documentation>An identifier used to number the contact detail elements in sequential order (i.e. 1..n) to be able to prioritize the order in which the contacts are to be contacted.</xs:documentation>
				<xs:appinfo>
					<mismo:DATATYPE>String</mismo:DATATYPE>
					<mismo:CONTEXT/>
					<mismo:PROCESSES>
						<mismo:PROCESS_AREA>Valuation</mismo:PROCESS_AREA>
						<mismo:PROCESS_AREA>Envelope</mismo:PROCESS_AREA>
					</mismo:PROCESSES>
					<mismo:LDD_NAME>Contact Detail Sequence Identifier</mismo:LDD_NAME>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="_ID" type="xs:ID"/>
	</xs:complexType>
